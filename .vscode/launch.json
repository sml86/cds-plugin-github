{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds serve",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": ["serve", "${workspaceFolder}/lib/github-service.cds"],
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "build"
    }
  ]
}
